{
  "name": "@pderas/dragon-drop",
  "version": "1.3.0",
  "description": "A simple to use Dragon Drop Library",
  "main": "dist/vue-dragon-drop.umd.min.js",
  "author": "Reed Jones <reed.jones@pderas.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src --ext .js,.vue",
    "lint:fix": "eslint src --ext .js,.vue --fix",
    "serve": "vue-cli-service serve",
    "watch": "vue-cli-service build --watch --target lib --name vue-dragon-drop ./src/main.js",
    "bundle": "vue-cli-service build --target lib --name vue-dragon-drop ./src/main.js",
    "build": "npm run lint && npm run bundle"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:PDERAS/dragon-drop.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [
    "dragon-drop",
    "dragon"
  ],
  "bugs": {
    "url": "https://github.com/PDERAS/dragon-drop/issues"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "devDependencies": {
    "@pderas/eslint-config": "^1.1.1",
    "@vue/babel-preset-app": "^4.2.3",
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-service": "^5.0.8",
    "eslint": "^6.8.0",
    "vue-template-compiler": "^2.6.11"
  },
  "contributors": [
    "Zahir Khan <zahir.khan@pderas.com"
  ]
}
